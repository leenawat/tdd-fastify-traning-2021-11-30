{
  "name": "tdd-fastify-traning-2021-11-30",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "test:coverage": "cross-env NODE_ENV=test jest --runInBand --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watch --runInBand",
    "test:watch-verbose": "cross-env NODE_ENV=test jest --watch --runInBand --verbose",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "dev:server": "cross-env NODE_ENV=develop nodemon src/server.ts",
    "dev": "tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"tsc --watch\" \"cross-env NODE_ENV=develop fastify start --ignore-watch=\\\"upload .next logs .ts$\\\" -w -l info -P dist/app.js\"",
    "migration:up": "db-migrate up",
    "migration:down": "db-migrate down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/jest": "^27.0.3",
    "bcryptjs": "^2.4.3",
