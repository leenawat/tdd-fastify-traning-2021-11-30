    "db-migrate-mysql": "^2.2.0",
    "fastify": "^3.0.0",
    "fastify-autoload": "^3.3.1",
    "fastify-cli": "^2.13.0",
    "fastify-guard": "^1.3.1",
    "fastify-jwt": "^4.0.1",
    "fastify-multer": "^2.0.2",
    "fastify-plugin": "^3.0.0",
    "fastify-sensible": "^3.1.0",
    "fastify-swagger": "^4.12.6",
    "fs-extra": "^10.0.0",
    "jest": "^27.4.0",
    "joi": "^17.4.2",
    "joi-to-json": "^2.1.1",
    "knex": "^0.95.14",
    "mime-types": "^2.1.34",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "pino-pretty": "^7.2.0",
    "ts-jest": "^27.0.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "0.0.40",
    "@types/fs-extra": "^9.0.13",
    "@types/knex": "^0.16.1",
    "@types/node": "^15.0.0",
    "@types/tap": "^15.0.0",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.3.0",
